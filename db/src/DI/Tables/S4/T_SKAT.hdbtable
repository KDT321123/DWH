COLUMN TABLE "BL.DI.Tables.S4::T_SKAT" (
    MANDT NVARCHAR(3) COMMENT'客户端 ',
SPRAS NVARCHAR(1) COMMENT'语言代码',
KTOPL NVARCHAR(4) COMMENT'科目表 ',
SAKNR NVARCHAR(10) COMMENT'总账科目编号',
TXT20 NVARCHAR(20) COMMENT'总账科目短文本 ',
TXT50 NVARCHAR(50) COMMENT'总账科目长文本 ',
MCOD1 NVARCHAR(25) COMMENT'匹配码搜索的搜索条件',
LAST_CHANGED_TS DECIMAL(15) COMMENT'简短格式的 UTC 时戳 (YYYYMMDDhhmmss)',
PRIMARY KEY(MANDT,SPRAS,KTOPL,SAKNR)
)