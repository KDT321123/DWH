COLUMN TABLE "BL.DI.Tables.S4::T_TCURR" (
    MANDT NVARCHAR(3) COMMENT'客户端',
    KURST NVARCHAR(4) COMMENT'汇率类型',
    FCURR NVARCHAR(5) COMMENT'从货币',
    TCURR NVARCHAR(5) COMMENT'最终货币',
    GDATU NVARCHAR(8) COMMENT'汇率有效起始日期',
    UKURS DECIMAL(9,5) COMMENT'汇率',
    FFACT DECIMAL(9) COMMENT'“从”货币单位的比率',
    TFACT DECIMAL(9) COMMENT'“到”货币单位的比率',
    PRIMARY KEY(MANDT,KURST,FCURR,TCURR,GDATU)
)