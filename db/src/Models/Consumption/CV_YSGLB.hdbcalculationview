    <filter>(&quot;RACCT&quot; LIKE '1122%' OR &quot;RACCT&quot; LIKE '1221%') AND &quot;AUGBL&quot; = '' AND &quot;DRCRK&quot; = 'S' AND &quot;BLDAT&quot; &lt;= &quot;CC_END_DATE&quot;</filter>
