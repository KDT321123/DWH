      <calculatedViewAttribute id="CC_END_DATE" datatype="NVARCHAR" length="8" expressionLanguage="SQL">
        <formula>TO_DATS(LAST_DAY(TO_DATE(&quot;CC_BEGIN_DATE&quot;, 'YYYYMMDD')))</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="CC_ZL" datatype="INTEGER" expressionLanguage="SQL">
        <formula>DAYS_BETWEEN(TO_DATE(&quot;BLDAT&quot;, 'YYYYMMDD'), TO_DATE(&quot;CC_END_DATE&quot;, 'YYYYMMDD'))</formula>
      </calculatedViewAttribute>
    </calculatedViewAttributes>
